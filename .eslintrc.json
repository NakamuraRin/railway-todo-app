{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/prop-types": 1
    }
}
